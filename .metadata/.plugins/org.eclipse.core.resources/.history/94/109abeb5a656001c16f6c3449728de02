package wrapperClass;

public class Program {

	public static void main(String[] args) {

		// Wrapper Class é o jeito natural da linguagem de fazer o Boxing e Unboxing, do tipo
		// classe, aceitando valores nulos também! .
		int x = 20;
		
		Integer obj = x; // Integer é o int.
		
		System.out.println(obj);

		int y = obj * 2;
	}
}
